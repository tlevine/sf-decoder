          <unit label="chapter" identifier="26" order_by="26" level="1">HOT WORK</unit>
          <unit label="section" identifier="2610" order_by="10" level="2">HOT WORK ON PIERS</unit>
          <unit label="division" identifier="2610.1" order_by="1" level="3">[For SF] Scope.</unit>
